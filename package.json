{
  "name": "v_mockup",
  "version": "1.0.0",
  "description": "recreating a mockup",
  "main": "index.js",
  "scripts": { 
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss css/style.comp.css -u autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-des/V-Mockup.git"
  },
  "author": "H-DES",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-des/V-Mockup/issues"
  },
  "homepage": "https://github.com/h-des/V-Mockup#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "concat": "^1.0.3",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
